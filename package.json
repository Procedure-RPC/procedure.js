  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^29.2.0",
    "@types/nanomsg": "^4.2.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typed-emitter": "^2.1.0",
    "typedoc": "^0.23.18",
    "typedoc-plugin-versions": "^0.2.1",
