  "funding": [
    "https://github.com/procedure-rpc/procedure.js?sponsor=1",
    {
      "type": "github",
      "url": "https://github.com/sponsors/toebeann"
    },
    {
      "type": "individual",
      "url": "https://paypal.me/tobeyblaber"
    }
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "posttest": "npm run format:check",
