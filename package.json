{
  "name": "@procedure-rpc/procedure.js",
  "version": "0.13.2",
  "title": "procedure.js",
  "description": "The simple RPC framework for Node.js.",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "author": "Tobey Blaber (https://github.com/toebeann)",
  "homepage": "https://procedure-rpc.github.io/procedure.js",
  "repository": {
    "type": "git",
